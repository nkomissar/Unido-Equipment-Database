<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping 
	PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.unido.eetdb.service.dbmodel">
	<class name="DbValueType" table="UNIDO_VALUE_TYPE" lazy="false">
	    <id name="id" column="VALUE_TYPE_ID">
        	<generator class="native"/>
    	</id>

    	<timestamp name="lastUpdatedDate" column="UPDATE_DATE" />

    	<property name="lastUpdatedBy" column="UPDATED_BY"/>
    	<property name="format" />
		<property name="type" />
		<property name="unitOfMeasure" column="UNIT_OF_MEASURE"/>
	</class>
</hibernate-mapping>