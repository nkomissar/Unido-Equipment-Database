<?xml version="1.0"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 6.0.0//EN" "http://www.liferay.com/dtd/liferay-friendly-url-routes_6_0_0.dtd">

<routes>
	<route>
		<pattern>/showTopic/{topicId:\d+}</pattern>
		<implicit-parameter name="action">showTopic</implicit-parameter>
	</route>
	<route>
		<pattern>/showEntity/{entityId:\d+}</pattern>
		<implicit-parameter name="action">showEntity</implicit-parameter>
	</route>
	<route>
		<pattern>/showSearch</pattern>
		<implicit-parameter name="action">showSearch</implicit-parameter>
	</route>
	<route>
		<pattern>/doSearch</pattern>
		<implicit-parameter name="action">doSearch</implicit-parameter>
	</route>
</routes>